{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "This repo contains a basic Node and Express app to get you started in constructing an API. To get started, clone this repo and run `yarn` in your terminal at the project root.",
  "main": "server.ts",
  "scripts": {
    "start": "npm run build && nodemon build/index.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "tsc": "tsc",
    "dev": "nodemon ./src/index.ts",
    "clean": "rimraf build/",
    "build": "yarn clean && npx tsc",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "lint": "eslint . --ext .ts",
    "migrate": "db-migrate up",
    "test": "set NODE_ENV=test && db-migrate --env test up  -c 10 && npx tsc && jasmine-ts && db-migrate --env test reset -c 10",
    "migration:run": "db-migrate up",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "add": "^2.0.6",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "supertest": "^6.1.6",
    "type": "^2.6.0",
    "types": "^0.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.12",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udacity/nd0067-c2-creating-an-api-with-postgresql-and-express-project-starter.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/udacity/nd0067-c2-creating-an-api-with-postgresql-and-express-project-starter/issues"
  },
